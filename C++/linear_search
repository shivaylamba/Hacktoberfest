#include<bits/c++.h>
int main()
{
//Pre existing array in which users data will be searched. 
 int arr[]={1,5,6,7,2,3,8};
int x,i=0,length=0,flag=0;
// Enter data to be searched.
cin>>x;
//size of array
length =sizeof(arr)/sizeof(arr[0])
for(i=0;i<length;i++)
{
  if(arr[i]==x;
  {
    cout>>"Found"
    flag=1;
  }
}
if(flag==0)
cout<<"Not Found";
return 0;
}
